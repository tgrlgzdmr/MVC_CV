
@{
    ViewBag.Title = "AddExperience";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}
<br />
<h4>Add New Experience Page</h4>
<br />
<form method="post">
    <label>Company</label>
    <input type="text" class="form-control" name="Title" placeholder="Please enter company name" />
    <br />
    <label>Title</label>
    <input type="text" class="form-control" name="Subtitle" placeholder="Please enter title" />
    <br />
    <label>Date</label>
    <input type="text" class="form-control" name="Date" placeholder="Please enter date" />
    <br />
    <label>Explanation</label>
    <textarea placeholder="Pease enter explanation" name="Explanation" class="form-control" rows="5"></textarea>
    <br />
    <button class="btn btn-success">Save</button>
</form>

