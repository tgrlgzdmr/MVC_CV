@using MVC_CV.Models.Entity
@model List<Tbl_Admin>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<br />
<h2>Admin List</h2>
<table class="table table-bordered">
    <tr>
        <th>ID</th>
        <th>User Name</th>
        <th>Delete</th>
        <th>Update</th>
    </tr>
    @foreach (var x in Model)
    {
        <tr>
            <td>@x.Id</td>
            <td>@x.UserName</td>
            <td><a href="/Admin/DeleteAdmin/@x.Id" class="btn btn-danger">Delete</a></td>
            <td><a href="/Admin/UpdateAdmin/@x.Id" class="btn btn-warning">Update</a></td>
        </tr>
    }

</table>
<a href="/Admin/AddAdmin/" class="btn btn-primary">Add New Admin</a>


